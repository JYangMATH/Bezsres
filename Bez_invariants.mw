<Group hide-input="false" labelreference="L30" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Sres_hb := proc(F,delta,x)
  local t,d0,d,delta0,M,R,c;
  t      := nops(F)-1;
  d0     := degree(F[1],x);
  d      := [seq(degree(F[i+1],x), i=1..t)];
  delta0 := max([seq(degree(F[i+1],x)+delta[i],i=1..t)])-d0;
  delta0 := max(delta0,1-add(delta));
  if delta0&lt;0 then return 0; fi;
  if add(delta)&gt;0 then
    M := Matrix_hb(F,delta,x);
    c := lcoeff(F[1],x)^(delta0-add(max(0,delta[i]+d[i]-d0), i=1..t));
    R := c*Determinant(M);
  else
    c := lcoeff(F[1],x)^(delta0-1);
    R := c*F[1];
  fi;
  
  R := collect(R,x);
  return R;
end:

sres_hb := proc(F,delta,x);
  local d,e;
  d  := degree(F[1],x);
  e  := (d+1)-add(delta);
  return factor(coeff(Sres_hb(F,delta,x),x,e-1));
end:

Matrix_J := proc(d0)
  local J,i;
  J   := Matrix(d0,d0,0);
  for i from 1 to d0 do
    J[i,  d0+1-i] :=  1;
  od;
  return J;
end:

Matrix_hb := proc(F,delta,x)
  local t,k,d0,i,P,R,X,M,J;
  t  := nops(F)-1;
  d0 := degree(F[1],x):
  J  := Matrix_J(d0);
  for k from 1 to t do
    R[k] :=convert(evalm(J&amp;*(BezoutMatrix(F[1], F[k+1], x))&amp;*J),Matrix):
    if k=1 then
      M := R[k];
    else
      M := &lt;M,R[k]&gt;;
    fi;
  od;
  M := SubMatrix(M,[seq(1+(i-1)*d0..delta[i]+(i-1)*d0, i=1..t)],[1..d0]);
  X := Matrix_X(d0,delta,d0,x);
  X := Transpose(X);
  M := &lt;M,X&gt;;
  return M;
end:</Text-field>
</Input>
</Group>
