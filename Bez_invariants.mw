<Group labelreference="L2" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Matrix_X := proc(d0,delta,h,x)
  local X,i;
  X   := Matrix(h,d0-add(delta),0);
  for i from 1 to d0-add(delta) do
    X[i,  i] :=  x;
    if i+1&lt;=h then 
      X[i+1,i] := -1;
    fi;
  od;
  for i from 1 to d0-add(delta) do
  od;
  return X;
end:</Text-field>
</Input>
</Group>
