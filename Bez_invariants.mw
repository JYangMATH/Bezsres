<Section collapsed="true" isCollapsible="true" drawButton="true" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title><Text-field style="Heading 2" layout="Heading 2">Barnett</Text-field></Title>
<Group labelreference="L42" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Sres_bar := proc(F,delta,x)
  local t,d0,delta0,M,R,c;
  t      := nops(F)-1;
  d0     := degree(F[1],x);
  delta0 := max([seq(degree(F[i+1],x)+delta[i],i=1..t)])-d0;
  delta0 := max(delta0,1-add(delta));
  if delta0&lt;0 then return 0; fi;
  if add(delta)&gt;0 then
    M := Matrix_bar(F,delta,x);
    c := lcoeff(F[1],x)^(delta0);
    R := c*Determinant(M);
  else
    c := lcoeff(F[1],x)^(delta0-1);
    R := c*F[1];
  fi;
  R := collect(R,x);
  return R;
end:

sres_bar := proc(F,delta,x);
  local d,e;
  d  := degree(F[1],x);
  e  := (d+1)-add(delta);
  return factor(coeff(Sres_bar(F,delta,x),x,e-1));
end:

Matrix_bar := proc(F, delta,x):
  local t,d0,C,lc,d,R,M,i,k,X;
  t   := nops(F)-1;
  d0  := degree(F[1],x);
  lc  := lcoeff(F[1],x);
  if lc=1 then
    C   := CompanionMatrix(F[1],x);
  else
    C   := CompanionMatrix(F[1],x)[1];
    for i from 1 to d0 do
      C[i,d0] := C[i,d0]/lc;
    od;
  fi;
  for k from 1 to t do
    d    := degree(F[k+1], x);
    R[k] := map(factor,add(coeff(F[k+1],x,i)*C^i.IdentityMatrix(d0),i=0..d));
  od;
  M := Matrix([seq(R[i], i=1..t)]);
  M := SubMatrix(M, [1..d0], [seq(1+(i-1)*d0..delta[i]+(i-1)*d0, i=1..t)]);
  X := Matrix_X(d0,delta,d0,x);
  M := Transpose(Matrix([M,X]));
  return M;
end:</Text-field>
</Input>
</Group>
