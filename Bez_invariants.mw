<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Sres_bez := proc(F,delta,x)
  local t,d0,delta0,M,R,c;
  t      := nops(F)-1;
  d0     := degree(F[1],x);
  delta0 := max([seq(degree(F[i+1],x)+delta[i],i=1..t)])-d0;
  delta0 := max(delta0,1-add(delta));
  if delta0&lt;0 then return 0; fi;
  if add(delta)&gt;0 then
    M := Matrix_bez(F,delta,x);
    c := lcoeff(F[1],x)^(delta0-add(delta));
    R := c*Determinant(M);
  else
    c := lcoeff(F[1],x)^(delta0-1);
    R := c*F[1];
  fi;
  
  R := collect(factor(R),x);
  return R;
end:

sres_bez := proc(F,delta,x);
  local d,e;
