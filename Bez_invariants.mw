<Group labelreference="L34" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">ss := proc(mu1,mu2)
  local i;
  if add(mu1)&gt;add(mu2) then return true fi;
  for i from 1 to nops(mu1) do
    if mu1[i] &gt; mu2[i] then return true  fi;
    if mu1[i] &lt; mu2[i] then return false fi;
  od;
  return false;
end:

Gen_deltas := proc(t,md)
  local deltas,T,ti;
  coeffs(expand((add(x[i],i=1..t)+1)^md),[x[i]$i=1..t],`T`);
  T := [T];
  T := map(ti-&gt;[seq(degree(ti,x[i]),i=1..t)],T);
  T := sort(T,ss);
  return T;
end:</Text-field>
</Input>
</Group>
