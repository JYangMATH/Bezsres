<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Sres_nhb := proc(F,delta,x)
  local t,d0,d,delta0,M,R,c;
  t      := nops(F)-1;
  d0     := degree(F[1],x);
  delta0 := max([seq(degree(F[i+1],x)+delta[i],i=1..t)])-d0;
  delta0 := max(delta0,1-add(delta));
  d      := [seq(degree(F[i+1],x), i=1..t)];
  if delta0&lt;0 then return 0; fi;
  if add(delta)&gt;0 then
    M := Matrix_nhb(F,delta,x);
    c := lcoeff(F[1],x)^(delta0-add(max(0,delta[i]+d[i]-d0), i=1..t));
    R := c*Determinant(M);
  else
    c := lcoeff(F[1],x)^(delta0-1);
    R := c*F[1];
  fi;
  
  R := collect(factor(R),x);
  return R;
end:

sres_nhb := proc(F,delta,x);
  local d,e;
  d  := degree(F[1],x);
  e  := (d+1)-add(delta);
  return factor(coeff(Sres_nhb(F,delta,x),x,e-1));
end:

Matrix_nhb := proc(F,delta,x)
  local t,k,d0,i,d,P,R,X,M,Q,N,U;
  t := nops(F)-1;
  d0 := degree(F[1],x):
  d  := [seq(degree(F[i+1],x), i=1..t)];
  for k from 1 to t do
    Q    := [seq(x^j*F[k+1], j=0..d0-d[k]-1)];
    N[k] := Matrix(d0-d[k],d0,(i,j)-&gt;coeff(Q[i],x,j-1)):
    P    := collect(simplify((F[1]*eval(F[k+1], x=y)-F[k+1]*eval(F[1],x=y))/(x-y)),{x,y},'distributed');
    R[k] := Matrix(d[k],d0,(i,j)-&gt;coeff(coeff(P,x,d[k]-i), y, j-1)):
    R[k] := Matrix(&lt;N[k],R[k]&gt;);
    R[k] := SubMatrix(R[k],[1..delta[k]],[1..d0]);
    if k=1 then
      M  := R[k];
    else
      M := &lt;M,R[k]&gt;;
    fi;
  od;
  X := Matrix_X(d0,delta,d0,x);
  M := &lt;M, Transpose(X)&gt;;
  return M;
end:</Text-field>
</Input>
</Group>
