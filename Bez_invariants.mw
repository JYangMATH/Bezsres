Matrix_bez := proc(F,delta,x)
  local t,k,d0,i,P,R,X,M;
  t := nops(F)-1;
  d0 := degree(F[1],x):
  for k from 1 to t do
    P    := collect(simplify((F[1]*eval(F[k+1], x=y)-F[k+1]*eval(F[1],x=y))/(x-y)),{x,y},'distributed');
    R[k] := Matrix(d0,d0,(i,j)-&gt;coeff(coeff(P,x,d0-j), y, i-1)):
  od;
  M := Matrix([seq(R[i], i=1..t)]);
  M := SubMatrix(M, [1..d0], [seq(1+(i-1)*d0..delta[i]+(i-1)*d0, i=1..t)]);
  X := Matrix_X(d0,delta,d0,x);
  M := Transpose(Matrix([M,X]));
  return M;
end:</Text-field>
